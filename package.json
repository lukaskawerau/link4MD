{
    "name": "wikilink4md",
    "displayName": "wikilink4md",
    "description": "wikilinks in markdownfiles",
    "version": "0.0.1",
    "publisher": "wangeleile",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.wikilink4md"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "extension.wikilink4md",
            "title": "wikilink"
        }],
		"keybindings": [{
				"command": "extension.wikilink4md",
				"key": "ctrl+alt+l",
				"mac": "ctrl+alt+l"
			}],		
		"configuration": {
				"type": "object",
				"title": "List Files To Open configuration",
				"properties": {
					"findFiles.fileIncludeGlob": {
						"type": "string",
						"default": "**",
						"description": "A glob pattern that defines the files to search for."
					},
					"findFiles.fileExcludeGlob": {
						"type": "string",
						"default": "**/node_modules/**",
						"description": "A glob pattern that defines files and folders to exclude."
					},
					"findFiles.maxResults": {
						"type": "number",
						"default": 1000,
						"description": "An upper-bound for the result."
					}
				}
        }
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}